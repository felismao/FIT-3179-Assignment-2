{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Los Angles Crime in 2020 - 2021",
    "width": 800,
    "height": 600,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://github.com/felismao/FIT-3179-Assignment-2/blob/13c5c6c39a074b79f1c0f6955d2dac868cbe8f83/LA%20Map.json",
      "format": {
            "type": "topojson",
            "feature": "LA Map"
          }
    },
    "transform": [
      {
      "lookup": "properties.name",
      "from": {
      "data": {
      "url": "https://github.com/felismao/FIT-3179-Assignment-2/blob/eed8bafb670b2d028d4cf63c4cab240fd0bb0d90/Crime_Data_from_2020_to_Present.csv"
      },
      "key": "AREA NAME",
      "fields": ["DR_NO"]
      }
      }
    ],
      "mark": {"type": "geoshape"},
      "encoding": {
      "color": {
      "field": "Active Cases",
      "type": "quantitative",
      "scale": {"type": "log"}
      },
      "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "Active", "type": "quantitative"}
      ]
      }
     }
     