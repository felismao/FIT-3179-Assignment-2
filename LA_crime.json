{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/felismao/FIT-3179-Assignment-2/main/LA%20Map.json",
        "format": {
          "type": "topojson",
          "feature": "LA Map"
        }
      },
       "projection": {
        "type": "mercator"
      },
      "mark": {
        "type": "geoshape",
        "fill": "lightgray",
        "stroke": "white"
      }
    },
     {
      "data": {
        "url": "https://raw.githubusercontent.com/felismao/FIT-3179-Assignment-2/main/Crime_Data_from_2020_to_Present.csv"
      },
        "projection": {
        "type": "mercator"
      },
      "mark": "circle",
      "encoding": {
        "longitude": {
          "field": "longitude",
          "type": "quantitative"
        },
         "latitude": {
          "field": "latitude",
          "type": "quantitative"
        },
        "size": {"value": 10},
        "color": {"value": "steelblue"}
      }
    }
  ]
}